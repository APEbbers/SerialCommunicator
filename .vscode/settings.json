{
    "python.defaultInterpreterPath": "D:/OneDrive/Programming/Projects/OctoPrint/OctoPrint/venv3/Scripts/python",
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--config",
        "black.toml"
